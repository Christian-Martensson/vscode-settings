	// }
	"getit": {
		"prefix": "getit",
		"body": "GetIt.I<$1>()",
		"description": "Basic GetIt"
	},
	"fpush": {
		"prefix": "npush",
		"body": "Navigator.push(context, MaterialPageRoute(builder: (context) => $1()));",
		"description": "Navigator Push"
	},
	"showDialog": {
		"prefix": "showd",
		"body": "showDialog<bool>(context: context,barrierDismissible: false,builder: (BuildContext context) {return $1();},);",
		"description": "Show Dialog"
	},
	"npop": {
		"prefix": "npop",
		"body": "Navigator.of(context).pop();",
		"description": "Navigator Pop"
	},
	"flatb": {
		"prefix": "flatb",
		"body": "FlatButton(child: Text('$1', style: TextStyle(fontSize: 16)),onPressed: () {$2},),",
		"description": "Navigator Pop"
	},
}